# Authentication (optional)
# Set to true to enable bearer token authentication
# If enabled, BEARER_TOKEN must be set
ENABLE_AUTH=False
BEARER_TOKEN=your_secure_bearer_token_here # Generate a secure token. Example: openssl rand -hex 32

# Rate Limiting Configuration
# Format: "number/time_unit" (e.g., "100/minute", "1000/hour")
# Default: 60 requests per minute per IP address
RATE_LIMIT=60/minute

# Redis Configuration (optional)
# Set to True if you want to enable Redis caching
ENABLE_REDIS=False
REDIS_URI=redis://localhost:6379
